  "scripts": {
    "dev": "vite",
    "dev-host": "vite --host",
    "dev-with-proxy": "concurrently \"npm run github-proxy\" \"npm run dev\"",
    "github-proxy": "node scripts/github-oauth-proxy.cjs",
    "build": "tsc && vite build",
    "backend-typecheck": "cd functions && tsc",
    "lint": "eslint ./src --fix",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "preview": "vite preview",
    "prepare": "husky",
    "precommit": "npm run lint && npm run format",
    "clean-deployments": "node --env-file=.env scripts/cleanupDeployments.js",
    "start-workers": "npx wrangler pages dev dist"
  },
